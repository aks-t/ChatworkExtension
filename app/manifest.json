{
  "name": "Chatwork Quick Info Input(改造版)",
  "short_name": "CW_QuickInfoInput(改造版)",
  "description": "https://github.com/aks-t/ChatworkExtension\nに感謝",
  "version": "1.0.0",
  "manifest_version": 3,
  "default_locale": "ja",
  "content_security_policy": { "extension_pages": "script-src 'self'; object-src 'self'" },
  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*.chatwork.com/*", "*://chatwork.com/*" ],
      "js": [ "scripts/contentscript.js" ]
    }
  ]
}
